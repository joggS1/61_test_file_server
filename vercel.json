{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "dist/server.js": {
      "memory": 3008,
      "maxDuration": 900
    }
  },
  "builds": [
      {
          "src": "dist/server.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["dist/**"] }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/server.js"
      }
  ]
}