{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
      {
          "src": "dist/server.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["dist/**"], "memory": 3008 }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/server.js"
      }
  ]
}